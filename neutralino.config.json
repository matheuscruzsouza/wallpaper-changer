{
  "applicationId": "js.neutralino.base.app",
  "defaultMode": "window",
  "port": 0,
  "url": "/resources/",
  "enableServer": true,
  "enableNativeAPI": true,
  "enableExtensions": true,
  "logging": {
    "enabled": false,
    "writeToLogFile": false
  },
  "nativeBlockList": [],
  "nativeAllowList": [
    "os.*"
  ],
  "globalVariables": {
    "DESKTOP_TEMPLATE_APP": "1.0.0.0"
  },
  "modes": {
    "window": {
      "title": "Wallpaper Changer",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/assets/icons/appIcon.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false
    }
  },
  "cli": {
    "binaryName": "wallpaper-changer",
    "resourcesPath": "/resources/",
    "clientLibrary": "/resources/neutralino.js",
    "binaryVersion": "4.8.0",
    "clientVersion": "4.8.0"
  },
  "extensions": [
    {
      "id": "js.neutralino.filesaver",
      "command": "/usr/bin/python3 ${NL_PATH}/extensions/py-dialog.py"
    }
  ]
}
